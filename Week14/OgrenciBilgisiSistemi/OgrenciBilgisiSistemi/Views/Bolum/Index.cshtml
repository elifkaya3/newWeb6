@model List<OgrenciBilgisiSistemi.Models.Entities.Bolum>
@{
    ViewData["Title"] = "Bölümler";
}
<h1 class="display-3">Bölüm Listesi</h1>
<hr />
<a asp-action="Create" class="btn btn-sm btn-primary">Create New Section</a><br /><br />

<table class="table table-dark">
    <thead>
        <tr>
            <th>Bölüm ID</th>
            <th>Bölüm Ad</th>
            <th>Bölüm Başkanı</th>
            <th>Dersler</th>
            <th>Öğrenciler</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.BolumId</td>
                <td>@item.BolumAd</td>
                <td>@item.BolumBaskanAd</td>
                <td>@item.Dersler</td>
                <td>@item.Ogrenciler</td>
                
            </tr>
        }
    </tbody>
</table>
